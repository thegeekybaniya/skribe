{
  "name": "@skribbl-clone/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "nx run-many -t build",
    "build:all": "nx run-many -t build --all",
    "build:affected": "nx affected -t build",
    "test": "nx run-many -t test",
    "test:all": "nx run-many -t test --all",
    "test:affected": "nx affected -t test",
    "test:watch": "nx run-many -t test --watch",
    "lint": "nx run-many -t lint",
    "lint:all": "nx run-many -t lint --all",
    "lint:affected": "nx affected -t lint",
    "lint:fix": "nx run-many -t lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "nx run-many -t type-check",
    "type-check:all": "nx run-many -t type-check --all",
    "clean": "nx reset",
    "dev:mobile": "nx serve mobile",
    "dev:server": "nx serve server",
    "build:mobile": "nx build mobile",
    "build:server": "nx build server",
    "build:types": "nx build types",
    "build:ui": "nx build ui",
    "test:mobile": "nx test mobile",
    "test:server": "nx test server",
    "test:types": "nx test types",
    "test:ui": "nx test ui",
    "graph": "nx graph",
    "affected:graph": "nx affected:graph"
  },
  "private": true,
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@expo/cli": "~0.21.8",
    "@nx/esbuild": "21.3.11",
    "@nx/eslint": "21.3.11",
    "@nx/eslint-plugin": "21.3.11",
    "@nx/expo": "^21.3.11",
    "@nx/jest": "21.3.11",
    "@nx/js": "21.3.11",
    "@nx/node": "^21.3.11",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@swc/jest": "~0.2.38",
    "@testing-library/jest-native": "~5.4.3",
    "@testing-library/react-native": "~12.9.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/node": "18.16.9",
    "@types/react": "~18.3.12",
    "babel-jest": "^30.0.2",
    "babel-preset-expo": "~12.0.1",
    "esbuild": "^0.19.2",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^10.0.0",
    "jest": "^30.0.2",
    "jest-environment-jsdom": "^30.0.2",
    "jest-environment-node": "^30.0.2",
    "jest-expo": "~52.0.2",
    "jest-util": "^30.0.2",
    "jsonc-eslint-parser": "^2.1.0",
    "nx": "21.3.11",
    "prettier": "^2.6.2",
    "react-test-renderer": "~18.3.1",
    "ts-jest": "^29.4.0",
    "ts-node": "10.9.1",
    "tslib": "^2.3.0",
    "typescript": "~5.8.2",
    "typescript-eslint": "^8.29.0",
    "verdaccio": "^6.0.5"
  },
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "dependencies": {
    "@expo/metro-config": "~0.19.4",
    "@expo/metro-runtime": "~4.0.0",
    "expo": "~52.0.11",
    "expo-splash-screen": "~0.29.13",
    "expo-status-bar": "~2.0.0",
    "express": "^4.21.2",
    "react": "~18.3.1",
    "react-dom": "~18.3.1",
    "react-native": "0.76.3",
    "react-native-svg": "~15.8.0",
    "react-native-svg-transformer": "~1.5.0",
    "react-native-web": "~0.19.13"
  },
  "nx": {
    "includedScripts": [],
    "targets": {
      "local-registry": {
        "executor": "@nx/js:verdaccio",
        "options": {
          "port": 4873,
          "config": ".verdaccio/config.yml",
          "storage": "tmp/local-registry/storage"
        }
      }
    }
  }
